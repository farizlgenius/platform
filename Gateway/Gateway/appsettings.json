{
  "ReverseProxy": {
    "Routes": {
      "identityRoute": {
        "ClusterId": "identityCluster",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      },
      "deviceRoute": {
        "ClusterId": "deviceCluster",
        "Match": {
          "Path": "/devices/{**catch-all}"
        }
      },
      "commandRoute": {
        "ClusterId": "commandCluster",
        "Match": {
          "Path": "/commands/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identityCluster": {
        "Destinations": {
          "identity": {
            "Address": "https://localhost:5001/"
          }
        }
      },
      "deviceCluster": {
        "Destinations": {
          "device": {
            "Address": "https://localhost:5002/"
          }
        }
      },
      "commandCluster": {
        "Destinations": {
          "command": {
            "Address": "https://localhost:5003/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}

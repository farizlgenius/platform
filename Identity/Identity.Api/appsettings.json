{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgresConnection": "Server=127.0.0.2;Port=5432;Database=identitydb;User Id=postgres;Password=password;",
    "SQLServerConnection": ""
  },
  "JwtSetting": {
    "Secret": "wVmLc8GTnNC7CeoLE27dp6bkm5drf8hHZJpzyf82jpQ=",
    "Issuer": "Honorsupplying",
    "Audience": "Client",
    "AccessTokenMinutes": 60
  },
  "Redis": {
    "Configuration": "127.0.0.1:6379"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp}] [{Level:u3}] [{ShortSourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/identity-log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp}] [{Level:u3}] [{ShortSourceContext}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
